<Tabs tabs="{{tabs}}" bindtabsItemChange="tabsItemChange">
  <view class="order_item" wx:for="{{orders}}" wx:key="order_id">
    <view class="order_no_wrap">
      <view>订单编号</view>
      <view>{{item.order_number}}</view>
    </view>
    <view class="order_no_wrap">
      <view>订单价格</view>
      <view>{{item.order_price}}</view>
    </view>
    <view class="order_no_wrap">
      <view>订单日期</view>
      <view>{{item.create_time_cn}}</view>
    </view>
  </view>
</Tabs>